        <script type="text/javascript">
            
            $(document).ready(function() {
                update_user();
            });
            $(document).ready(function() {
                $(document).click(function(event) { 
                    if($(event.target).parents().index($('#header-box')) == -1 &&
                        $(event.target).parents().index($('#header_side')) == -1 &&
                        $(event.target).parents().index($('.navbar')) == -1) {
                        if($('#header-box').is(":visible")) {
                            $('#header-box').slideUp();
                            $('.overlay').fadeOut('fast');
                        }
                    }        
                });
                $(document).keyup(function () {
                    if($('#header-box').is(":visible") && event.keyCode == 27) {
                        $('#header-box').slideUp();
                        $('.overlay').fadeOut('fast');
                    }
                });
                
                $('#center_state').change(function () {
                    var state = $('#center_state').val();
                    var search = '';
                    if ($('#center_plate').val()) {
                        search = $('#center_plate').val();
                    }
                    $('#center_plate_img').attr('src','/plate/get_plate_image?plate='+ search + '&state=' + state);
                });
                $('#center_plate').keyup(function () {
                    var state = $('#center_state').val();
                    var search = '';
                    if ($('#center_plate').val()) {
                        search = $('#center_plate').val();
                    }
                    $('#center_plate_img').attr('src','/plate/get_plate_image?plate='+ search + '&state=' + state);
                });
                
                
                $('#right_state').change(function () {
                    var state = $('#right_state').val();
                    var search = '';
                    if ($('#right_plate').val()) {
                        search = $('#right_plate').val();
                    }
                    $('#right_plate_img').attr('src','/plate/get_plate_image?plate='+ search + '&state=' + state);
                });
                $('#right_plate').keyup(function () {
                    var state = $('#right_state').val();
                    var search = '';
                    if ($('#right_plate').val()) {
                        search = $('#right_plate').val();
                    }
                    $('#right_plate_img').attr('src','/plate/get_plate_image?plate='+ search + '&state=' + state);
                });
                
                $('#message').blur(function() {
                    if ($('#message').val() == '') {
                        $('#post_box').slideUp();
                    } 
                });
                
                $('#post_upload_img_field').blur(function () {
                    if ($('post_upload_img_field').val() == '') {
                        $('post_upload_img').slideUp();
                    } 
                });
            });
            
            function upload_img() {
                $('#post_upload_img_prog').fadeIn();
                var myxhr = function() { 
                    myXhr = $.ajaxSettings.xhr();
                    if(myXhr.upload){
                        myXhr.upload.addEventListener('progress', progress_img, false);
                    }
                    return myXhr;
                }
                var success = function (data) {
                    $('#post_upload_img_preview').attr('src', data.data);
                    $('#post_upload_img_url').val(data.data);
                    $('#post_upload_img_prog').fadeOut();
                }
                upload_file($('#post_upload_img'), success, myxhr);
            }
            function progress_img(e) {
                if(e.lengthComputable){
                    $('#post_upload_img_prog').attr({
                        value:e.loaded,
                        max:e.total
                    });
                }
            }
            
            
            function upload_vid() {
                $('#post_upload_vid_prog').fadeIn();
                $('#post_upload_vid_btn').fadeOut();
                var myxhr = function() { 
                    myXhr = $.ajaxSettings.xhr();
                    if(myXhr.upload){
                        myXhr.upload.addEventListener('progress', progress_vid, false);
                    }
                    return myXhr;
                }
                var success = function (data) {
                    $('#post_upload_vid_url').val(data.data);
                    $('#post_upload_vid_prog').fadeOut();
                }
                upload_file($('#post_upload_vid'), success, myxhr);
            }
            function progress_vid(e) {
                if(e.lengthComputable){
                    $('#post_upload_vid_prog').attr({
                        value:e.loaded,
                        max:e.total
                    });
                }
            }
            
            $('#post_link_url').keyup(function () {
                var url = $('#post_link_url').val();
                if (validateURL(url)) {
                    util_get_link_info(success, error, url)
                    function success(data) {
                        console.log(data);
                        var html_content = '';
                        if (data.img) {
                            html_content += '<div class="row"><div class="span3 offset1">';
                            html_content += '<img src="' + data.img + '" />';
                            html_content += '</div></div>';
                        }
                        if (data.title) {
                            html_content += '<div class="row"><div class="span3 offset1">';
                            html_content += '<div>' + data.title + '</div>';
                            html_content += '</div></div>';
                        }
                        if (data.desc) {
                            html_content += '<div class="row"><div class="span3 offset1">';
                            html_content += '<div>' + data.desc + '</div>';
                            html_content += '</div></div>';
                        }
                        html_content += '<div class="row">&nbsp;</div>';
                        $('#post_link_preview').html(html_content);
                    }
                    function error() {
                        
                    }
                }
            })
            
            function validateURL(textval) {
                var urlregex = new RegExp(
                "^(http|https|ftp)\://([a-zA-Z0-9\.\-]+(\:[a-zA-Z0-9\.&amp;%\$\-]+)*@)*((25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9])\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[0-9])|([a-zA-Z0-9\-]+\.)*[a-zA-Z0-9\-]+\.(com|edu|gov|int|mil|net|org|biz|arpa|info|name|pro|aero|coop|museum|[a-zA-Z]{2}))(\:[0-9]+)*(/($|[a-zA-Z0-9\.\,\?\'\\\+&amp;%\$#\=~_\-]+))*$");
                return urlregex.test(textval);
            }
        </script>
